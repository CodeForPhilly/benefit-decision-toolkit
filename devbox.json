{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.7/.schema/devbox.schema.json",
  "packages": [
    "maven@latest",
    "quarkus@latest",
    "jdk21@latest",
    "firebase-tools@latest",
    "google-cloud-sdk@latest",
    "nodejs@22"
  ],
  "env_from": ".env",
  "shell": {
    "init_hook": [
      "[ -f .devboxrc ] && . ./.devboxrc",
      "echo 'Welcome to devbox!'"
    ],
    "scripts": {
      "setup": [
        "bin/setup"
      ]
    }
  }
}
